@startuml

scale 3

class Animal {
    makeSound()
}
class Cat extends Animal {
    makeSound()
}

class Vehicle {
    drive()
}

class Car extends Vehicle {

    drive()
}

 class Shape {
    getArea()
} 

class Rectangle extends Shape {
    -length;
    -width;
    __construct()
    getArea()
}

class Employee {
    -salary;
    __construct()
    work()
    getSalary()
}

class HRManager extends Employee {
    __construct()
    work()
    addEmployee()
}

class BankAccount {
    -accountNumber;
    -balance;
    __construct()
    deposit(amount)
    withdraw(amount)
    getBalance()
}

class SavingsAccount extends BankAccount {
    __construct()
    withdraw(amount) 
}

